{
  "name": "jambo2",
  "private": true,
  "version": "0.9.25",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "server": "tsx src/multiplayer/server.ts",
    "ai:bench:short": "tsx scripts/ai-benchmark.ts short",
    "ai:bench:medium": "tsx scripts/ai-benchmark.ts medium",
    "ai:bench:long": "tsx scripts/ai-benchmark.ts long",
    "version:bump": "tsx scripts/bump-version.ts patch",
    "version:minor": "tsx scripts/bump-version.ts minor",
    "version:major": "tsx scripts/bump-version.ts major"
  },
  "dependencies": {
    "mongodb": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.21.0",
    "ws": "^8.19.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
